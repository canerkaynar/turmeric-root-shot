{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalDecorators": true
        }
    },
    "rules": {
        "no-console": [
            1,
            {
                "allow": [
                    "warn",
                    "error"
                ]
            }
        ],
        "react/prop-types": [
            2
        ],
        "react/prefer-es6-class": [
            2
        ],
        "react/no-unused-prop-types": [
            2
        ],
        "react/prefer-stateless-function": [
            1
        ],
        "react/self-closing-comp": [
            2
        ]
    },
    "extends": [
        //"airbnb"
    ],
    "plugins": [
        "react"
    ]
}